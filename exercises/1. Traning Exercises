s = ''
#Find the length of the sequence and print something like: ‘The length of the sequence is X’#
print(len(s))
print("The length of the sequence is", len(s))
#Find out how many different nucleotides are there (A, G, T…)#
m=s.replace('N','')
len(set(m))
#Count the number of A's and T's#
print(s.count("A") + s.count("T"))
#Count the number of G's and C's#
print(s.count("G") + s.count("C"))
#Calculate the GC content (% GC versus the total number of nucleotides)#
print(((s.count("G") + s.count("C"))*100)/(len(s)))
